pico-8 cartridge // http://www.pico-8.com
version 38
__lua__
function varsetup()
	mx,my=0,0
	cx,cy=0,0
	make_player()
	max_x = 16
	max_y = 16
	min_x = -1
	min_y = -1
	n = 0
	prevlevel = 0
	currlevel = 0
	starttime = time()
	currtime = time()
	currtimer = 10
end

function _init()
 varsetup()
	intro()
end

function intro()
local text = "miny"
sfx(4)
for i=1,2 do
		cls()
		print(text, hcenter(text), vcenter(text), 8)
		for x=4,10 do
			flip()
		end
			cls()
		print(text, hcenter(text), vcenter(text), 11)
		for x=4,10 do
			flip()
		end
			cls()
		print(text, hcenter(text), vcenter(text), 12)
		for x=4,10 do
			flip()
		end
	end
	cls()
	print(text, hcenter(text), vcenter(text), 14)
	for x=4,30 do
		flip()
	end
end

function _update()
	move_player()
	update_map()
end

function _draw()
	cls()
	update_camera()
	map(mx,my,mx*8,my*8,16,16)
	spr(p.sprite,p.x*8,p.y*8)
	if(n!=0) then
		currtime = time()
		print(currtimer - (currtime - starttime),hcenter("      "),cy,8)
		if(currtime - starttime > currtimer) then
			die()
		end
	end
	currlevel = n
	print(tostring(currlevel),cx+1,cy+1,11)
end

function get_level()
	while (n==prevlevel) do
		n = 1 + flr(rnd(3))
	end
	sfx(1)
	cls()
	if(n == 1) then
		p.x=8
		p.y=22
		max_x = 16
		max_y = 32
		min_x = -1
		min_y = 15
	end
	if(n == 2) then
		p.x=8
		p.y=37
		max_x = 16
		max_y = 48
		min_x = -1
		min_y = 31
	end
	if(n == 3) then
		p.x=8
		p.y=52
		max_x = 16
		max_y = 64
		min_x = -1
		min_y = 47
	end
	prevlevel = n
	starttime = time()
end

function levelchange(ax,ay) 
	local map_sprite=mget(ax,ay)
	local flag = fget(map_sprite)
	return flag == 2
end

function make_player()
	p={}
	p.x = 8
	p.y = 8
	p.sprite = 1
end

function move_player()
	local new_x, new_y = p.x, p.y
	if(btnp(0)) then
		new_x-=1
		p.sprite = 4
	end
 if(btnp(1)) then
  new_x+=1
  p.sprite = 1
 end
	if(btnp(2)) then
	 new_y-=1
	 p.sprite = 1
	end
	if(btnp(3)) then
	 new_y+=1
	 p.sprite = 1
	end
	if(new_x!=p.x or new_y!=p.y) then
		if(can_move(new_x,new_y) and new_x < max_x and new_y<max_y and new_y>min_y and new_x>min_x) then
			p.x = new_x
			p.y = new_y
		end
		if(levelchange(new_x,new_y)and new_x < max_x and new_y<max_y and new_y>min_y and new_x>min_x) then
			get_level()
		end
		if(checkback(new_x,new_y)) then
			goback()
		end
		if(checkdeath(new_x,new_y)) then
			die()
		end
		if(not can_move(new_x,new_y) or new_x > max_x or new_y>max_y or new_y<min_y or new_x<min_x) then
			sfx(5)
		end
	end
end

function can_move(x,y)
	local map_sprite=mget(x,y)
	local flag = fget(map_sprite)
	return flag != 1
end

function update_map()
	mx=flr(p.x/16)*16
	my=flr(p.y/16)*16
end

function update_camera()
	cx=mx*8
	cy=my*8
	camera(cx,cy)
end

function goback()
	cls()
	sfx(3)
	varsetup()
	return true
end

function checkback(ax,ay) 
	local map_sprite=mget(ax,ay)
	local flag = fget(map_sprite)
	return flag == 4
end

function die()
	cls()
	spr(13,p.x*8,p.y*8)
	sfx(0)
	for x=4,16 do
		flip()
	end
	print("you died", (p.x - 1)*8, (p.y - 1)*8, 8)
	sfx(2)
	for x=4,100 do
		flip()
	end
	varsetup()
end

function checkdeath(ax,ay)
	local map_sprite =mget(ax,ay)
	local flag = fget(map_sprite)
	return flag == 3
end

function hcenter(s)
	return 64-#s*2
end

function vcenter(s)
	return 61
end


